FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p_rgh;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField     uniform 0;

boundaryField
{    
    inletOutlet
    {
        type            fixedFluxPressure;
    }
    
    bottom
    {
        type            fixedFluxPressure;
    }
    
    top
    {
        type            totalPressure;
        p0              uniform 0;
        U               U;
        phi             phi;
        rho             rho;
        psi             none;
        gamma           1;
        value           uniform 0;
    }
    
    oversetPatch
    {
        type            overset;
        value           uniform 0;
    }    
    
    FBWSide
    {
        type            overset;
        value           uniform 0;
    }   
    
    FBW
    {
        type            fixedFluxPressure;
    } 
}


