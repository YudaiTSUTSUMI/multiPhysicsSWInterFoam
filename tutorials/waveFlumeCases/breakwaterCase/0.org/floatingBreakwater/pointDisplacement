FoamFile
{
    version     2.0;
    format      ascii;
    class       pointVectorField;
    object      pointDisplacement;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 0 0 0 0 0];

internalField     uniform (0 0 0);

boundaryField
{  
    inletOutlet
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }    
    
    bottom
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }
    
    top
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }
    
    oversetPatch
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }
    
    FBWSide
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }
    
    FBW
    {
        type                       calculated;
        value                     uniform (0 0 0);
    }
}


