FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "";
    object      surfaceFeatureExtractDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

cylinder.stl         
{
    extractionMethod    extractFromSurface;
    extractFromSurfaceCoeffs
    {
        includedAngle  150;
    }
    subsetFeatures
    {
        nonManifoldEdges       no;
        openEdges              yes;
    }
    writeObj               no;
}

