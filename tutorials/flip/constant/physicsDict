FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      physicsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

frictionMode  average; //minimum,average, default: multiply 

nIterBullet   20;

//implicitEccentric true;

objects
{
    floor
    {
        objName         floor;
        objShape        box;
        
        Lx      10;
        Ly      10;
        Lz      10;
        
        position    (0 0 -5.1); 
        
        rhoSolid        0; // staticBody
        
        restitution     0.0;
        friction      0.5;
        
        writeVTK    true; //defalt
    }
    
    box
    {
        objName         box;
        objShape        box;

        position    (0 0 0.125);
        
        // Cuboid dimensions
        Lx              0.05;
        Ly              0.05;
        Lz              0.15;

        // Density of the solid
        rhoSolid        1e-6; // to keep dynamic
        
        restitution     0.0;
        friction      0.5;
        
        fluidForceRelaxation 0.6;
        fluidForceDamping    1;
        
        initialVelocity     (0 0 2);
        initialAngularVelocity   (0 21.98 0);

        constraints
        {
             /*planeConstraint
             {
               bulletMotionConstraint   plane;
               normal                   (1 0 1);
             }*/
             
             /*axisConstraint
             {
               bulletMotionConstraint   axis;
               axis                              (0 1 0);
             }*/  
        }
        
        writeVTK    true;
    }

    boxSolid //for comparison
    {
        objName         boxSolid;
        objShape        box;

        position    (-0.3 0 0.125);
        
        // Cuboid dimensions
        Lx              0.05;
        Ly              0.05;
        Lz              0.15;

        // Density of the solid
        rhoSolid        200;
        
        restitution     0.0;
        friction      0.5;
        
        fluidForceRelaxation 0.6;
        fluidForceDamping    1;
        
        initialVelocity     (0 0 2);
        initialAngularVelocity   (0 21.98 0);

        constraints
        {
        }
        
        writeVTK    true;
    }
}

joints
